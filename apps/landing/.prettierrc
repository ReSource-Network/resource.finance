{
  "semi": false,
  "trailingComma": "all",
  "printWidth": 150,
  "tabWidth": 2,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": ["*.yml", "*.yaml"],
      "options": {
        "bracketSpacing": false
      }
    }
  ]
}
